{
  "name": "jollywheels",
  "version": "0.9.52",
  "description": "This game is not for kids, its physics / gore and fun!",
  "main": "./build/index.html",
  "repository": "https://smerik@bitbucket.org/smerik/jollywheels.git",
  "author": "Erik Sombroek <eriksombroek@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:deleteold": "rm -rf ./build",
    "build:webpack": "env NODE_ENV=production yarn webpack",
    "build:obfuscate": "node obfuscateScript.js",
    "build:hash": "node hashfiles.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:webpack": "webpack --watch",
    "watch:server": "yarn run server",
    "watch:code": "code .",
    "optimise": "bash image-opti.sh static",
    "blueprints": "npm-run-all blueprints:*",
    "blueprints:deleteold": "rm -rf ./static/assets/blueprints",
    "blueprints:makenew": "mkdir ./static/assets/blueprints",
    "blueprints:build": "node generateBlueprints.js",
    "audiosprites-core": "npm-run-all audiosprites-core:*",
    "audiosprites-core:sfx": "audiosprite --output ./static/assets/audio/sfx-hq --format howler2 --export ogg,mp3 ./asset_src/audio/core-sfx/*.ogg",
    "audiosprites-core:copy": "cp -R ./static/assets/audio ./build/assets",
    "audiosprites-core:compressogg": "ffmpeg -i ./static/assets/audio/sfx-hq.ogg -c:a libvorbis -ab 32k -ar 22050 ./static/assets/audio/sfx-lq.ogg",
    "audiosprites-core:compressmp3": "ffmpeg -i ./static/assets/audio/sfx-hq.mp3 -q:a 9 -ac 1 ./static/assets/audio/sfx-lq.mp3",
    "audiosprites-pack1": "npm-run-all audiosprites-pack1:*",
    "audiosprites-pack1:sfx": "audiosprite --output ./static/assets/audio/pack1-hq --format howler2  --export ogg,mp3 ./asset_src/audio/pack1-sfx/*.ogg",
    "audiosprites-pack1:copy": "cp -R ./static/assets/audio ./build/assets",
    "audiosprites-pack1:compress": "ffmpeg -i ./static/assets/audio/pack1-hq.ogg -c:a libvorbis -ab 32k -ar 22050 ./static/assets/audio/pack1-lq.ogg",
    "audiosprites-pack1:compressmp3": "ffmpeg -i ./static/assets/audio/pack1-hq.mp3 -q:a 9 -ac 1 ./static/assets/audio/pack1-lq.mp3",
    "audiosprites-pack2": "npm-run-all audiosprites-pack2:*",
    "audiosprites-pack2:sfx": "audiosprite --output ./static/assets/audio/pack2-hq --format howler2  --export ogg,mp3 ./asset_src/audio/pack2-sfx/*.ogg",
    "audiosprites-pack2:copy": "cp -R ./static/assets/audio ./build/assets",
    "audiosprites-pack2:compress": "ffmpeg -i ./static/assets/audio/pack2-hq.ogg -c:a libvorbis -ab 32k -ar 22050 ./static/assets/audio/pack2-lq.ogg",
    "audiosprites-pack2:compressmp3": "ffmpeg -i ./static/assets/audio/pack2-hq.mp3 -q:a 9 -ac 1 ./static/assets/audio/pack2-lq.mp3",
    "server": "http-server build"
  },
  "dependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "animejs": "^3.0.1",
    "audiosprite": "^0.7.2",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.0.2",
    "core-js": "^3.8.3",
    "css-loader": "^0.28.9",
    "extend": "^3.0.2",
    "file-loader": "^6.0.0",
    "gamestats.js": "^0.9.1",
    "html-minifier": "^4.0.0",
    "http-server": "^0.11.1",
    "idb-keyval": "^5.0.4",
    "lz-string": "^1.4.4",
    "md5-file": "^5.0.0",
    "nanoid": "^1.2.3",
    "npm-run-all": "^4.1.2",
    "pixi-filters": "^2.6.1",
    "pixi-particles": "^4.3.0",
    "pixi.js": "6.0.0-rc.2",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.32.8",
    "sass-loader": "10.1.1",
    "self": "^1.0.0",
    "simplebar": "^5.3.0",
    "simplify-js": "^1.2.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "web-social-share": "^6.4.1",
    "webpack": "^4.43.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.12"
  }
}
